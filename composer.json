{
    "name": "isidromerayo/agile_design_with_tdd",
    "description": "Diseño Ágil con TDD por Carlos Blé",
    "keywords": ["test", "composer", "Helper", "TDD", "PHPUnit", "Test Doubles", "Mock", "Stub", "xUnit", "Agil"],
    "homepage": "http://www.dirigidoportests.com/el-libro",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Isidro Merayo Castellano",
            "homepage": "https://bitbucket.org/isidromerayo"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "pimple/pimple": "1.*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.x-dev",
        "mockery/mockery": "dev-master",
        "phpunit/DbUnit": "1.2.*",
        "ext-sqlite3": "*",
        "sensiolabs/security-checker": "dev-master"
    },
    "suggest": {
        "ext/xdebug": "Debugger and profiling"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}